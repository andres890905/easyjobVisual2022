@model IEnumerable<easyjob22.Models.Zona>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreZona)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdusuariosNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreZona)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdusuariosNavigation.Idusuarios)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdZona">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdZona">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdZona">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
