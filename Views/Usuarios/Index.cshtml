                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaRegistro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdRolesNavigation.IdRoles)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSucursalNavigation.IdSucursal)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idusuarios">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idusuarios">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idusuarios">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
